<body>
    <div id="app">
        <div class="task_list_wrapper">
            <div class="task_list_header">
                <img src="./images/task_list/bg.jpg" alt="">
            </div>
            <ul class="task_list_content">
                <li>
                    <div class="task_list_content_div_left f">
                        <div class="time_box">
                            <p>9:00-10:00</p>
                        </div>
                        <h1 class="task_title">早安问候</h1>
                        <p class="task_state">定时问候 唤醒用户</p>
                    </div>
                    <div class="task_list_content_div_right r">
                        <div class="btn">
                            <p>已过期</p>
                        </div>
                    </div>
                </li>
                <li>
                    <div class="task_list_content_div_left f">
                        <div class="time_box">
                            <p>10:45-11:30</p>
                        </div>
                        <h1 class="task_title">早市商品发布</h1>
                        <p class="task_state">定时完成转发 培养用户习惯</p>
                    </div>
                    <div class="task_list_content_div_right r">
                        <div class="btn_one">
                            <p>已完成</p>
                        </div>
                    </div>
                </li>
                <li>
                    <div class="task_list_content_div_left f">
                        <div class="time_box">
                            <p>18:45-19:30</p>
                        </div>
                        <h1 class="task_title">晚市商品发布</h1>
                        <p class="task_state">定时完成转发 培养用户习惯</p>
                    </div>
                    <div class="task_list_content_div_right r">
                        <div class="btn_two">
                            <p>待开始</p>
                        </div>
                    </div>
                </li>
                <li>
                    <div class="task_list_content_div_left f">
                        <div class="time_box">
                            <p>14:00-22:00</p>
                        </div>
                        <h1 class="task_title">爆品推荐</h1>
                        <p class="task_state">每日热销爆品 增加销量神器</p>
                    </div>
                    <div class="task_list_content_div_right r">
                        <div class="btn_three">
                            <p>已完成</p>
                        </div>
                    </div>
                </li>
            </ul>
        </div>

    </div>
    <script>
        new Vue({
            el: "#app",
            data: {},
            mounted() {

            },
            methods: {

            }
        });
