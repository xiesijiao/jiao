body{
    background-color:#d74763;
}
.lay_content{
    margin:4rem auto;
    width:5rem;
    height:1rem;
    font-size:0.32rem;
    text-align:center;
    font-family:PingFangSC-Medium, PingFang SC;
    font-weight:bold;
    color:#fff;
}
.lay_img{
    width:1.6rem;
    height:1.1rem;
    margin:0 auto;
}
.lay_img > img{
    width:1.6rem;
    height:1.1rem;
}
*{ margin:0; padding:0; -webkit-tap-highlight-color:transparent; }
a{ text-decoration:none; color:black; }
li{ list-style:none; }
#app{ -webkit-user-select:none; -webkit-text-size-adjust:none; width:100%; height:100%; position:relative; overflow:hidden; margin:0 auto; }
[v-cloak]{
    display:none;
}
.f{
    float:left;
}
.r{
    float:right;
}