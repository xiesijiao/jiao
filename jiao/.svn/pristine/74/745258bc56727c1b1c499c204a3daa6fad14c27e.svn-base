@charset "utf-8";
/* CSS Document */
* { margin: 0;padding: 0;
}
body{ background-color: #e7e7e7;}
a {text-decoration: none;color: black;}
li {list-style: none;}
#app {font-size: 12px;-webkit-user-select: none;-webkit-text-size-adjust: none;height: 100%;width:100%;position: relative;}
/* registOne css*/
.choose_box{width: 4.12rem;height: 2.41rem;margin: 50% auto; display: flex;justify-content: space-between;}
.choose_box_one{width: 1.58rem;height: 2.41rem;text-align: center;}
.choose_box_one_img{width: 1.58rem;height: 1.58rem;border-radius: 50%;}
.chooseBoxOneP{font-size: 0.28rem;color: #686868;margin-top: 0.45rem;}
/* registTwo css */
.regist_box{width: 6.4rem;height: 12.5rem;margin: 0.5rem auto 0rem;}
.regist_box_p{font-size: 0.42rem;font-family:PingFangSC-Medium, PingFang SC;font-weight:bold;color: rgba(51, 51, 51, 1);line-height: 0.59rem; display: inline-block;}
.regist_content{width: 5.88rem;height: 10.89rem;margin: 0.36rem auto 0rem;}

.form_div{width: 5.88rem;height: 1.6rem;border-bottom: 1px solid #929293;display: flex;}
.form_div_left{width: 0.6rem;height: 1.6rem;}
.name_img{width: 0.32rem;height: 0.32rem;margin: 0.5rem 0.17rem;}
.form_div_right{ width: 5rem;height: 1rem; margin-top: 0.46rem;}
.form_div_right_p{font-family: PingFangSC-Regular,PingFang SC; font-size: 0.24rem;color: #999999;}
.form_div_right_inp{ margin-top: 0.12rem; width: 3.2rem; height: 0.6rem; border: none; background-color: #e7e7e7;outline: none; font-size: 0.28rem; font-family:PingFangSC-Medium, sans-serif;}
.form_div_right>span{color: #d65243; display: inline;}
.verification_code_btn{width :1.7rem; height:0.4rem;font-size: 0.22rem;color: #d65243; border: none;background-color: #e7e7e7;outline: none; display: inline-block;}
.foot_p{ margin: 0.33rem 0 0 0.81rem;font-size: 0.2rem;color: #929293;font-family: '微软雅黑';}
.submit_btn{text-align: center;width: 5.88rem; height: 0.88rem;font-size: 0.36rem;color: white;background-color: #d65243;border-style: none;border-radius: 0.12rem;margin-top: 0.58rem;line-height: 0.88rem;}
[v-cloak]{
    display: none;
}
/* registTwo 弹窗1 */
.card{width: 100%;height: 100%;background: rgba(0,0,0,0.6);position: fixed;top: 0;left: 0;z-index: 999;display: flex;justify-content: center;align-items: center;}
.card_succeed{width: 5.06rem;height: 7.3rem;}
.card_succeed_top_img{width: 5.06rem;height: 3.65rem;border-radius: 0.12rem;}
.card_succeed_top_img_one{width: 5.06rem;height: 5.13rem;border-radius: 0.12rem;}
.card_succeed_top_div{width: 5.06rem;height: 1.7rem;background-color: #3c3a3b;margin-top: -0.28rem;border-radius: 0.12rem;} 
.card_succeed_top_div_p>h1{font-size: 0.32rem;color: white;font-family: PingFangSC-Regular,PingFang SC;text-align: center; padding-top: 0.47rem;padding-bottom: 0.22rem;}
.card_succeed_top_div_p>p{font-size: 0.2rem;color: #d6ca04;display: block;text-align: center;}
.card_succeed_top_btn{width: 5.06rem;height: 1.06rem;font-size: 0.32rem;border-radius: 0.12rem;background-color: white;border-style: none;color: #ff9297;margin-top: 0.86rem;}
/* registThree */
.form_div_right_se{background-color: #e7e7e7;-webkit-appearance: none;border-style: none; width: 3.2rem; height: 0.6rem; margin-top: 0.08rem; font-size: 0.28rem;}
.form_div_right_se:active,.form_div_right_se:focus{outline: none;}
.form_div_right_two{position: relative;}
.trans{width: 0.3rem;height: 0.3rem;position: absolute;top:0.6rem;left:4.5rem}
/* regist_Manager_edit */
.header_div {height: 1rem;width: 100%;background-color: white;line-height: 0.88rem; display: flex;justify-content: center;align-items: center;}
.header_div_content{height: 0.8rem;width: 7rem;display: flex;align-items: center;}
.header_div_content>img{width: 0.4rem;height: 0.4rem;}

.form_div_right_inp_style{
    color: #999999;
}